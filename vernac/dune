(library
 (name vernac)
 (synopsis "Coq's Vernacular Language")
 (public_name coq.vernac)
 (wrapped false)
 (libraries tactics parsing))

(rule
 (targets g_proofs.ml)
 (mode promote-until-clean)
 (deps (:mlg-file g_proofs.mlg))
 (action (run coqpp %{mlg-file})))

(rule
 (targets g_vernac.ml)
 (mode promote-until-clean)
 (deps (:mlg-file g_vernac.mlg))
 (action (run coqpp %{mlg-file})))
