(rule
 (targets Ipat.timing Ipat.glob Ipat.vos Ipat.vo)
 (deps ../../theories/Init/Prelude.vo)
 (action (chdir ../../. (run coqc -boot -R theories Coq -Q user-contrib/Ltac2ssr Ltac2ssr -I plugins/ring -I plugins/nsatz -I plugins/derive -I plugins/ltac2_ltac1 -I plugins/firstorder -I plugins/ltac2 -I plugins/rtauto -I plugins/ltac -I plugins/ssr -I plugins/btauto -I plugins/micromega -I plugins/funind -I plugins/extraction -I plugins/syntax -I plugins/ssrmatching -I plugins/cc -w +default -q -w -deprecated-native-compiler-option -native-compiler off -time-file user-contrib/Ltac2ssr/Ipat.timing %{dep:Ipat.v}))))
(rule
 (targets Tactic.timing Tactic.glob Tactic.vos Tactic.vo)
 (deps ../../theories/Init/Prelude.vo ../../user-contrib/Ltac2ssr/Ipat.vo)
 (action (chdir ../../. (run coqc -boot -R theories Coq -Q user-contrib/Ltac2ssr Ltac2ssr -I plugins/ring -I plugins/nsatz -I plugins/derive -I plugins/ltac2_ltac1 -I plugins/firstorder -I plugins/ltac2 -I plugins/rtauto -I plugins/ltac -I plugins/ssr -I plugins/btauto -I plugins/micromega -I plugins/funind -I plugins/extraction -I plugins/syntax -I plugins/ssrmatching -I plugins/cc -w +default -q -w -deprecated-native-compiler-option -native-compiler off -time-file user-contrib/Ltac2ssr/Tactic.timing %{dep:Tactic.v}))))
(install
 (section lib_root)
 (package coq-stdlib)
 (files (Ipat.v as coq/user-contrib/Ltac2ssr/Ipat.v)
        (Ipat.glob as coq/user-contrib/Ltac2ssr/Ipat.glob)
        (Ipat.vos as coq/user-contrib/Ltac2ssr/Ipat.vos)
        (Ipat.vo as coq/user-contrib/Ltac2ssr/Ipat.vo)))
(install
 (section lib_root)
 (package coq-stdlib)
 (files (Tactic.v as coq/user-contrib/Ltac2ssr/Tactic.v)
        (Tactic.glob as coq/user-contrib/Ltac2ssr/Tactic.glob)
        (Tactic.vos as coq/user-contrib/Ltac2ssr/Tactic.vos)
        (Tactic.vo as coq/user-contrib/Ltac2ssr/Tactic.vo)))
