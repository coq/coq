(install (section doc) (package coq-refman)
 (files (index.html as refman/index.html)
        (.buildinfo as refman/.buildinfo)
        (coq-exnindex.html as refman/coq-exnindex.html)
        (user-extensions as refman/user-extensions)
        (zebibliography.html as refman/zebibliography.html)
        (coq-optindex.html as refman/coq-optindex.html)
        (credits.html as refman/credits.html)
        (search.html as refman/search.html)
        (license.html as refman/license.html)
        (addendum as refman/addendum)
        (coq-cmdindex.html as refman/coq-cmdindex.html)
        (searchindex.js as refman/searchindex.js)
        (objects.inv as refman/objects.inv)
        (_images as refman/_images)
        (_sources as refman/_sources)
        (practical-tools as refman/practical-tools)
        (genindex.html as refman/genindex.html)
        (coq-tacindex.html as refman/coq-tacindex.html)
        (language as refman/language)
        (_static as refman/_static)
        (proof-engine as refman/proof-engine)
        ))