	$(MKDIR) $(FULLBINDIR)
	$(INSTALLBIN) $(COQINTERFACE) $(FULLBINDIR)

install-pcoq-files::
	$(MKDIR) $(FULLCOQLIB)/contrib/interface
	$(INSTALLLIB) $(INTERFACERC) $(FULLCOQLIB)/contrib/interface

