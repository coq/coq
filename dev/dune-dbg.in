#!/usr/bin/env bash

# Run in a proper install dune env.
case $1 in
    checker)
        exe=_build/default/checker/main.bc
        ;;
    *)
        exe=_build/default/topbin/coqtop_byte_bin.bc
        ;;
esac

ocamldebug $(ocamlfind query -recursive -i-format coq.top_printers) -I +threads -I dev $exe
